{
	"name": "Microsoft Forms",
	"version": "1.0.0",
	"description": "Helper for Microsoft Forms.",
	"permissions": ["webRequest", "scripting", "storage", "tabs"],
	"host_permissions": ["https://forms.office.com/*"],
	"manifest_version": 3,
	"author": "Sampurna Chapagain",
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon/16.png",
			"24": "icon/24.png",
			"32": "icon/32.png"
		}
	},
	"icons": {
		"16": "icon/16.png",
		"32": "icon/32.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
	}
}
